    "import numpy as np\n",
    "import scipy\n",
    "from scipy.optimize import minimize\n",
    "\n",
    "def p1(x):\n",
    "    return np.array([0, x, x])\n",
    "\n",
    "def p2(x):\n",
    "    return np.array([x, 1, x])\n",
    "\n",
    "def d(X):\n",
    "    a = p1(X[0])\n",
    "    b = p2(X[1])\n",
    "    return np.linalg.norm(a-b)\n",
    "    \n",
    "minimize(d, [0,0])\n"
   ]
