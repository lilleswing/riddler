    "import math\n",
    "def ac(d):\n",
    "    r = d / 2\n",
    "    return math.pi * r * r\n",
    "\n",
    "outer = 0.5 * (ac(22) - ac(18)) + 0.5 * ac(4)\n",
    "inner = 0.5 * (ac(12) - ac(8)) + 0.5 * ac(4)\n",
    "outer + inner"
   ]
